#Debian Menu{{{ 
#If you edit this file, you will probably not want to remove the
#following section, which makes menus of Debian apps available.
preproc: \
	if tty|egrep -q "tty[0-9]|vc/[0-9]|console"; then 	\
		cat /var/lib/pdmenu/pdmenurc.vc;		\
	elif [ "$DISPLAY" ]; then				\
		cat /var/lib/pdmenu/pdmenurc.x11;		\
	else							\
		cat /var/lib/pdmenu/pdmenurc;			\
	fi

